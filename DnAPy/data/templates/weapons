`damagetype` varchar(45) DEFAULT 'Piercing',
`force` int(11) DEFAULT '0',
`forcemodifier` decimal(10,0) DEFAULT '1',
`parry` int(11) DEFAULT '1',
`cost` int(11) DEFAULT '6',
`weight` int(11) DEFAULT '200',
`minstr` int(11) DEFAULT '0',
`range` varchar(45) DEFAULT NULL,
`costreload` varchar(45) DEFAULT NULL