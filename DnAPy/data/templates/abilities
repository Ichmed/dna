`name` varchar(45) NOT NULL,
`cost` int(11) DEFAULT NULL,
`skill` varchar(45) DEFAULT NULL,
`leveling` varchar(45) DEFAULT NULL,
`manacost` tinytext,
`actionpoints` tinytext,
`type` tinytext,
`range` tinytext,
`implement` tinytext,
`ingredients` mediumtext,
`duration` tinytext,
`staminacost` tinytext,
`display_name` tinytext,
`parent` varchar(45),
`chance` tinytext,
`requires` tinytext,
`force` tinytext,
`conditions` mediumtext,
`requires_raw` tinytext,
`class` varchar(45) DEFAULT NULL,
`root` varchar(45) DEFAULT NULL,
`index` int(11) DEFAULT NULL,
`equipment` tinytext,
`effect` mediumtext,
`result` mediumtext,
`quality` tinytext,
`flavor` mediumtext,
`tags` mediumtext,
UNIQUE(`name`, `parent`)
